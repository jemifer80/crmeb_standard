APP_DEBUG = true[APP]DEFAULT_TIMEZONE = Asia/ShanghaiAPP_KEY = #APP_KEY#[DATABASE]TYPE = mysqlHOSTNAME = #DB_HOST#HOSTPORT = #DB_PORT#USERNAME = #DB_USER#PASSWORD = '#DB_PWD#'DATABASE = #DB_NAME#PREFIX = #DB_PREFIX#CHARSET = utf8mb4DEBUG = true[REDIS]REDIS_HOSTNAME = #RB_HOST#PORT = #RB_PORT#REDIS_PASSWORD = '#RB_PWD#'SELECT = #RB_SELECT#[CACHE]DRIVER = redisPREFIX = #CACHE_PREFIX#TAG_PREFIX = #CACHE_TAG_PREFIX#[LANG]default_lang = zh-cn